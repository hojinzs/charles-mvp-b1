# Database Configuration
DATABASE_URL=postgresql://crawler:crawler123@localhost:5432/crawler_db

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Server Configuration
PORT=3000
NODE_ENV=development

# Worker Configuration
WORKER_CONCURRENCY=2

# Scheduler Configuration
SCHEDULER_CRON=* * * * *
SCEDULER_INTERVAL_MS=60000

# Puppeteer
# PUPPETEER_EXECUTABLE_PATH=/usr/bin/google-chrome

# Proxy Configuration (BrightData)
# Leave empty to disable proxy
PROXY_HOST=brd.superproxy.io
PROXY_PORT=33335
PROXY_USER=brd-customer-hl_xxxxx-zone-isp
PROXY_PASS=your_proxy_password

# CloudWatch Configuration (for ECS Auto Scaling)
# Set to 'true' on EC2 Scheduler to publish queue metrics to CloudWatch
CLOUDWATCH_METRICS_ENABLED=false
AWS_REGION=ap-northeast-2
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
